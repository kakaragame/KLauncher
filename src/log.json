{
  "default_loggers": [
    {
      "levels": [
        "DEBUG",
        "INFO",
        "WARN",
        "ERROR"
      ],
      "targets": [
        {
          "type": "console",
          "properties": {
            "format": "[%module%] %level_{color='true'}%: %message%"
          }
        }
      ]
    }
  ],
